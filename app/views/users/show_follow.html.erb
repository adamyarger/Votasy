<div class="bigbox">
 	<div class="box">
 		<div class="follow-container">
	 		<ul>
		    <% @users.each do |user| %>
	    		<li>
	    			<span class="avatar-small"><%= link_to image_tag(user.avatar.url(:thumb)), user %></span>
	    			<span class="follow-name"><%= user.name %></span>
	    		</li>
		    <% end %>
		  </ul>
	  </div>
	</div>
</div>

